{
  "name": "Grand Warden",
  "description": "Secure Password Manager - Your digital guardian for all your passwords",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background-bundle.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },
  "options_page": "test-extension.html",
  "icons": {
    "16": "hello_extensions.png",
    "32": "hello_extensions.png",
    "48": "hello_extensions.png",
    "128": "hello_extensions.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "identity"
  ],
  "host_permissions": [
    "https://*.mystenlabs.com/*",
    "https://fullnode.testnet.sui.io/*",
    "https://faucet.testnet.sui.io/*",
    "https://accounts.google.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["test-extension.html", "test-bg.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "key": "",
  "oauth2": {
    "client_id": "36098691154-oi9pm05ra1f70ov7pb43t9c0cg94isuo.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile",
      "openid"
    ]
  }
}